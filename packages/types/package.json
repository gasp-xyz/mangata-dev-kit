{
  "name": "@mangata-finance/types",
  "version": "1.1.0",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@mangata-finance/type-definitions": "2.0.0-rc.8"
  },
  "peerDependencies": {
    "@polkadot/api": "^10.9.1"
  },
  "type": "module",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false
}
