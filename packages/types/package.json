{
  "name": "@mangata-finance/types",
  "version": "2.0.0",
  "description": "Polkadot.js type definitions for Mangata Network",
  "author": "Mangata Finance <solutions@mangata.finance>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mangata-finance/mangata-dev-kit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mangata",
    "types"
  ],
  "license": "GPL-3.0",
  "devDependencies": {
    "@mangata-finance/type-definitions": "2.0.0"
  },
  "peerDependencies": {
    "@polkadot/api": "^10.9.1"
  },
  "type": "module",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false
}
